{
  "name": "@ui5/webcomponents-react-base",
  "version": "0.20.3",
  "description": "Base for ui5-webcomponents-react",
  "main": "index.esm.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "homepage": "https://sap.github.io/ui5-webcomponents-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/ui5-webcomponents-react.git",
    "directory": "packages/base"
  },
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "sideEffects": [
    "./polyfill/*",
    "core-js/**/*",
    "intersection-observer"
  ],
  "scripts": {
    "clean": "rimraf Device hooks polyfill styling utils index.esm.js index.d.ts dist",
    "build:rollup": "rollup -c rollup.config.mjs",
    "build:polyfills": "tsc ./src/polyfill/*.ts --outDir ./polyfill --skipLibCheck",
    "build": "npm-run-all -s build:rollup build:polyfills",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir . --removeComments false"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "core-js": "3.20.0",
    "intersection-observer": "0.12.0",
    "resize-observer-polyfill": "1.5.1",
    "smoothscroll-polyfill": "0.4.4"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@ui5/webcomponents-base": "~1.0.2",
    "react": "^16.8.0 || ^17.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@ui5/webcomponents-base": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "ui5wcr-codemod": "./bin/codemod.js"
  },
  "files": [
    "bin",
    "codemods",
    "Device",
    "dist",
    "hooks",
    "polyfill",
    "styling",
    "types",
    "utils",
    "CHANGELOG.md",
    "index.d.ts",
    "index.esm.js",
    "LICENSE",
    "NOTICE.txt",
    "README.md"
  ]
}
