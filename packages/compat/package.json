{
  "name": "@ui5/webcomponents-react-compat",
  "version": "2.13.2",
  "description": "React Wrapper for UI5 Web Components Compat package",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./styles.css": "./dist/css/index.css",
    "./dist/*": "./dist/*",
    "./dist/*.js": "./dist/*.js",
    "./enums/*": {
      "types": "./dist/enums/*.d.ts",
      "default": "./dist/enums/*.js"
    },
    "./VersionInfo": {
      "types": "./dist/generated/VersionInfo.d.ts",
      "default": "./dist/generated/VersionInfo.js"
    },
    "./Loader": {
      "types": "./dist/components/Loader/index.d.ts",
      "default": "./dist/components/Loader/index.js"
    },
    "./OverflowToolbarButton": {
      "types": "./dist/components/OverflowToolbarButton/index.d.ts",
      "default": "./dist/components/OverflowToolbarButton/index.js"
    },
    "./OverflowToolbarToggleButton": {
      "types": "./dist/components/OverflowToolbarToggleButton/index.d.ts",
      "default": "./dist/components/OverflowToolbarToggleButton/index.js"
    },
    "./Table": {
      "types": "./dist/components/Table/index.d.ts",
      "default": "./dist/components/Table/index.js"
    },
    "./TableCell": {
      "types": "./dist/components/TableCell/index.d.ts",
      "default": "./dist/components/TableCell/index.js"
    },
    "./TableColumn": {
      "types": "./dist/components/TableColumn/index.d.ts",
      "default": "./dist/components/TableColumn/index.js"
    },
    "./TableGroupRow": {
      "types": "./dist/components/TableGroupRow/index.d.ts",
      "default": "./dist/components/TableGroupRow/index.js"
    },
    "./TableRow": {
      "types": "./dist/components/TableRow/index.d.ts",
      "default": "./dist/components/TableRow/index.js"
    },
    "./Toolbar": {
      "types": "./dist/components/Toolbar/index.d.ts",
      "default": "./dist/components/Toolbar/index.js"
    },
    "./ToolbarSeparator": {
      "types": "./dist/components/ToolbarSeparator/index.d.ts",
      "default": "./dist/components/ToolbarSeparator/index.js"
    },
    "./ToolbarSpacer": {
      "types": "./dist/components/ToolbarSpacer/index.d.ts",
      "default": "./dist/components/ToolbarSpacer/index.js"
    }
  },
  "homepage": "https://sap.github.io/ui5-webcomponents-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/ui5-webcomponents-react.git",
    "directory": "packages/compat"
  },
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "sideEffects": [
    "./dist/Assets.js",
    "./dist/json-imports/*"
  ],
  "scripts": {
    "clean": "rimraf dist tmp",
    "build:css": "postcss --base src --dir dist/css src/**/*.css",
    "build:css-bundle": "node ../../config/merge-css-modules.js",
    "build:version-info": "node ../../scripts/generate-version-info.js",
    "watch:css": "yarn build:css --watch"
  },
  "dependencies": {
    "clsx": "2.1.1"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "@ui5/webcomponents-compat": "~2.14.0",
    "@ui5/webcomponents-react": "~2.14.0",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    },
    "@ui5/webcomponents-base": {
      "optional": true
    },
    "@ui5/webcomponents-icons": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "NOTICE.txt",
    "README.md"
  ]
}
