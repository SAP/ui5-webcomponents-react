{
  "name": "@ui5/webcomponents-react",
  "version": "0.5.1-rc.1",
  "description": "React Wrapper for UI5 Web Components and additional components",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/ui5-webcomponents-react.git",
    "directory": "packages/main"
  },
  "main": "index.cjs.js",
  "module": "index.esm.js",
  "types": "index.d.ts",
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "private": false,
  "scripts": {
    "generateWebComponents": "ts-node -O '{\"module\": \"commonjs\"}' -r esm ./scripts/wrapperGeneration/generateWebComponentWrappers --onlyStopForMerge",
    "build": "webpack --config ./scripts/wrapperGeneration/webpack.config.js",
    "postbuild": "rollup -c rollup.config.js"
  },
  "dependencies": {
    "@ui5/webcomponents": "1.0.0-rc.2",
    "@ui5/webcomponents-react-base": "0.5.1-rc.1",
    "lodash.debounce": "^4.0.8",
    "react-table": "6.10.0",
    "react-toastify": "^5.0.1"
  },
  "devDependencies": {
    "diff": "^4.0.1",
    "inquirer": "^6.3.1"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  }
}
