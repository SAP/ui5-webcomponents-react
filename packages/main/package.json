{
  "name": "@ui5/webcomponents-react",
  "version": "0.27.5",
  "description": "React Wrapper for UI5 Web Components and additional components",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://sap.github.io/ui5-webcomponents-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/ui5-webcomponents-react.git",
    "directory": "packages/main"
  },
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "sideEffects": [
    "./dist/Assets.js",
    "./dist/json-imports/*"
  ],
  "scripts": {
    "clean": "rimraf dist tmp wrappers",
    "build": "npm-run-all -s build:i18n build:tsc build:wrappers",
    "build:tsc": "tsc",
    "build:wrappers": "node scripts/build-wrappers.mjs && tsc --project tsconfig.wrappers.json && rimraf tmp",
    "build:i18n": "node scripts/generateI18n.mjs"
  },
  "dependencies": {
    "@ui5/webcomponents-react-base": "^0.27.5",
    "clsx": "1.2.1",
    "lit-html": "^2.0.1",
    "react-jss": "^10.9.0",
    "react-table": "7.8.0",
    "@tanstack/react-virtual": "3.0.0-beta.18"
  },
  "peerDependencies": {
    "@types/react": "*",
    "@types/react-dom": "*",
    "@ui5/webcomponents": "~1.6.0",
    "@ui5/webcomponents-base": "~1.6.0",
    "@ui5/webcomponents-fiori": "~1.6.0",
    "@ui5/webcomponents-icons": "~1.6.0",
    "react": "^16.14.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.14.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    },
    "@types/react-dom": {
      "optional": true
    },
    "@ui5/webcomponents-base": {
      "optional": true
    },
    "@ui5/webcomponents-icons": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "wrappers",
    "CHANGELOG.md",
    "jestSetup.js",
    "LICENSE",
    "NOTICE.txt",
    "README.md"
  ]
}
