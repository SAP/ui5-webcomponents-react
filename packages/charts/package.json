{
  "name": "@ui5/webcomponents-react-charts",
  "version": "2.0.1",
  "description": "Charts for ui5-webcomponents-react",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json",
    "./dist": "./dist/index.js",
    "./dist/*": "./dist/*",
    "./dist/*.js": "./dist/*.js",
    "./styles.css": "./dist/css/index.css"
  },
  "homepage": "https://sap.github.io/ui5-webcomponents-react",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/ui5-webcomponents-react.git",
    "directory": "packages/charts"
  },
  "author": "SAP SE (https://www.sap.com)",
  "license": "Apache-2.0",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build:css": "postcss --base src --dir dist/css src/**/*.css",
    "build:css-bundle": "node ../../config/merge-css-modules.js",
    "build:version-info": "node ../../scripts/generate-version-info.js",
    "watch:css": "yarn build:css --watch"
  },
  "dependencies": {
    "clsx": "2.1.1",
    "react-content-loader": "7.0.2",
    "recharts": "2.12.7"
  },
  "peerDependencies": {
    "@ui5/webcomponents-react": "~2.0.0",
    "@ui5/webcomponents-react-base": "~2.0.0",
    "react": "^18"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "NOTICE.txt"
  ]
}
