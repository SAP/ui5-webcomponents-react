{
  "name": "ui5-webcomponents-react",
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "license": "Apache-2.0",
  "scripts": {
    "start": "lerna run build:i18n && start-storybook --no-manager-cache -p 6006",
    "build": "node scripts/build.js",
    "build:storybook": "lerna run build:i18n && build-storybook -o .out",
    "postinstall": "rimraf node_modules/@types/mocha",
    "pretest": "rimraf temp && lerna run build",
    "test:jest": "jest --config=config/jest.config.js --coverage",
    "test:cypress": "cypress run --component",
    "test:cypress:open": "CYPRESS_COVERAGE=false cypress open --component --browser chrome",
    "test": "yarn test:cypress && yarn test",
    "test:combine-reports": "node scripts/combineTestReports.mjs",
    "clean": "lerna run clean --stream && rimraf temp",
    "prettier:all": "prettier --write --config ./prettier.config.cjs \"packages/**/*.{js,jsx,ts,tsx,mdx}\"",
    "lint": "eslint packages",
    "lerna:version-dryrun": "lerna version --conventional-graduate --no-git-tag-version --no-push",
    "create-webcomponents-wrapper": "node --experimental-json-modules ./packages/main/scripts/create-web-components-wrapper.mjs && node --experimental-json-modules ./scripts/generate-theming-parameters.js",
    "examples:recreate-seed": "rimraf examples/seed-test && yarn create react-app examples/seed-test --template file:./packages/cra-template-seed",
    "examples:start-seed": "cd examples/seed-test && SKIP_PREFLIGHT_CHECK=true yarn start",
    "prepare": "husky install && lerna run build:i18n",
    "chromatic": "cross-env STORYBOOK_ENV=chromatic npx chromatic --build-script-name build:storybook"
  },
  "dependencies": {
    "@storybook/addon-a11y": "6.5.10",
    "@storybook/addon-essentials": "6.5.10",
    "@storybook/builder-webpack5": "6.5.10",
    "@storybook/manager-webpack5": "6.5.10",
    "@storybook/react": "6.5.10",
    "@storybook/theming": "6.5.10",
    "@ui5/webcomponents": "1.7.0",
    "@ui5/webcomponents-fiori": "1.7.0",
    "@ui5/webcomponents-icons": "1.7.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-syntax-highlighter": "^15.2.1",
    "tocbot": "^4.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@cypress/code-coverage": "^3.10.0",
    "@semantic-release/github": "^8.0.0",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-17": "npm:@testing-library/react@^12.1.4",
    "@testing-library/react-hooks": "^7.0.0",
    "@types/node": "^16.0.0",
    "@types/react": "^18.0.6",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.30.0",
    "@ui5/webcomponents-tools": "1.7.0",
    "@vitejs/plugin-react": "^2.0.1",
    "chromatic": "^6.5.3",
    "cypress": "^10.7.0",
    "dedent": "^0.7.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.31.7",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "glob": "^8.0.1",
    "handlebars": "^4.7.7",
    "husky": "^8.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.1",
    "jest-canvas-mock": "^2.2.0",
    "jest-environment-jsdom": "^29.0.1",
    "lerna": "^5.2.0",
    "lint-staged": "^13.0.0",
    "mkdirp": "^1.0.4",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react-16": "npm:react@^16.14.0",
    "react-17": "npm:react@^17.0.2",
    "react-dom-16": "npm:react-dom@^16.14.0",
    "react-dom-17": "npm:react-dom@^17.0.2",
    "resize-observer-polyfill": "1.5.1",
    "rimraf": "^3.0.1",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "turndown": "^7.0.0",
    "typescript": "4.8.2",
    "vite": "^3.0.9",
    "vite-plugin-istanbul": "^3.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mdx}": [
      "prettier -config ./prettier.config.cjs --write"
    ]
  },
  "type": "module",
  "browserslist": {
    "production": [
      "> 0.2%",
      "not dead",
      "not op_mini all",
      "not IE 11",
      "not Edge <= 18",
      "not ios_saf < 12"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 edge version",
      "last 1 safari version",
      "last 1 firefox version"
    ]
  },
  "nyc": {
    "report-dir": "temp/cypress-coverage",
    "temp-dir": "temp/.nyc_output",
    "reporter": [
      "json"
    ],
    "include": [
      "packages/{base,charts,main}/src/**/*.{ts,tsx}"
    ],
    "exclude": [
      "**/src/interfaces/*",
      "**/src/enums/*",
      "**/*.stories.tsx",
      "**/*.jss.ts",
      "**/*.test.{ts,tsx}",
      "**/node_modules/**",
      "packages/*/src/index.ts",
      "packages/main/src/components/AnalyticalTable/types/*"
    ]
  }
}
